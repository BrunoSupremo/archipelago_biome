	"level_1":{
		"default_fish":{
			"_underline_prefixes":"are just for comments/instructions",

			"alias":"archipelago_biome:food:fish",
			"_alias":"alias of the desired item that can be fished",

			"weight":100,
			"_weight":"a number > 0. relative chance of getting this item instead of the others",

			"_max_limit":"optional. a number > 0, how many times the fisher can get this item",

			"_bulletin":{
				"_comment":"optional. shows a notification when the fisher get this item.",
				"_title":"the notification and bulletin title",
				"_message":"whatever you want to appear as text in the bulletin",
				"_image":"address of the image you want displayed, probably a portrait of the item"
			},

			"_is_biome":[
				"optional. it makes the item only available for the listed biomes",
				"stonehearth:biome:temperate",
				"stonehearth:biome:desert"
			],
			"_is_not_biome":[
				"optional. it prohibits the item for the listed biomes",
				"stonehearth:biome:temperate",
				"stonehearth:biome:desert"
			],

			"_is_kingdom":[
				"optional. it makes the item only available for the listed kingdoms",
				"stonehearth:kingdoms:ascendancy",
				"rayyas_children:kingdoms:rayyas_children"
			],
			"_is_not_kingdom":[
				"optional. it prohibits the item for the listed kingdoms",
				"stonehearth:kingdoms:ascendancy",
				"rayyas_children:kingdoms:rayyas_children"
			],

			"(is_) and (is_not_)":[
				"are mutually exclusive, you can't use both",
				"when none is used, the item will simple have no restriction",
			],

			"effort":120,
			"_effort":"a number > 0. how long in (game) minutes it takes to fish this item",

			"xp":15,
			"_xp":"a number > 0. rewarded xp after the action. ~100xp is required to reach level 2"
		},
