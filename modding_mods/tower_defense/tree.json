{
   "components": {
      "mixintypes": {
         "destination": "remove"
      },
      "render_info": {
         "scale": 0.09
      },
      "mob": {
         "mixintypes": {
            "align_to_grid": "remove"
         },
         "model_origin": { "x": 0, "y": 0, "z": 0 },
         "region_origin": { "x": 0.5, "y": 0, "z": 0.5 }
      },
      "region_collision_shape": {
         "mixintypes": {
            "region": "override"
         },
         "region": [
            {
               "min": { "x": 0, "y": 0, "z": 0 },
               "max": { "x": 1, "y": 1, "z": 1 }
            }
         ]
      }
   }
}